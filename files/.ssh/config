Host eosnode-1 eosnode-1.colo.seagate.com
    HostName eosnode-1.colo.seagate.com
    User root
    UserKnownHostsFile /dev/null
    StrictHostKeyChecking no
    IdentityFile /root/.ssh/id_rsa_prvsnr
    IdentitiesOnly yes
    LogLevel ERROR
    BatchMode yes

